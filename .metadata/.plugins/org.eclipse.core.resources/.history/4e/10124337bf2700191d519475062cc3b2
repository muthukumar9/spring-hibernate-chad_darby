@Component
public class CRMLoggingAspect {

	//setup logger
	private Logger myLogger = Logger.getLogger(getClass().getName());
	
	//setup pointcut declarations
	@PointCut("")
	private void forControllerPackage() { }
	
	//add @Before advice
	
	//add @AfterReturning advice
}
